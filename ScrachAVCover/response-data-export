<script type="text/javascript">
    //<!--
    $(function() {
    var UA = navigator.userAgent;
    if(
        ((UA.indexOf('iPhone') !== -1)
        || (UA.indexOf('Android') !== -1)
        || (UA.indexOf('Windows Phone') !== -1)
        || (UA.indexOf('iPad') !== -1)
        || (UA.indexOf('Touch') !== -1))
    ) {
        //touchdevice
       var $popup = $('#rental-localnav li.popup');
       var $popupUl = $('ul', $popup);

       $popupUl.hide();
       $popup.click(function() {
           $popupUl.toggle();
           $(this).toggleClass('off');
       });
    }
    $('#rental-localnav li.popup').each(function(i){
        $(this).mouseover(function(){
            var navWidth = $(this).width();
            $('#rental-localnav li.popup ul').eq(i).css({minWidth: navWidth});
        });
    });
});
    //-->
</script>
