<script>
$(window).on('load', (function(){
    //イージングのプラグイン
    $.easing.easeOutQuint = function(x, t, b, c, d){
       return c*((t=t/d-1)*t*t*t*t + 1) + b;
    };

    var openwindowHeight, selectwindowHeight, selectwindowTop, faheight, fukiarrowTop, documentTop, that, timerID;
    //ライトボックスの画像を先読みしておく
    var img01 = new Image();
    var img02 = new Image();
    var img03 = new Image();
    var img04 = new Image();
    var img05 = new Image();
    var img06 = new Image();
    img01.src = "//p.dmm.co.jp/p/list/bg_selectwin_bo.png";
    img02.src = "//p.dmm.co.jp/p/list/bg_selectwin_to.png";
    img03.src = "//p.dmm.co.jp/p/list/ico_close.gif";
    img04.src = "//p.dmm.co.jp/p/list/bg_selectwin.png";
    img05.src = "//p.dmm.co.jp/p/list/arrow_selectwin.png";
    img06.src = "//p.dmm.co.jp/p/list/ico_loader.gif";

    // 連続クリック防止フラグ
    var flag = false;

    $(".openwindow").click(function () {
        if (flag) {
            return false;
        }

        //先に開いているライトボックスがあれば閉じる
        if ($("#side-l .selectwindow").length) {
            $("#side-l .selectwindow").remove();
        }

        flag = true;

        //this=a.openwindowを違うスコープでも使えるよう格納
        that = $(this);

        //「もっと見る」にロードアイコンを付ける
        that.addClass("load");

        function setWindowPosition() {
            //ライトボックスの表示位置を計算
            openwindowHeight = that.offset();
            selectwindowHeight = $("#side-l .selectwindow").height();
            selectwindowTop = openwindowHeight.top - (selectwindowHeight / 2);
            fukiarrowTop = (selectwindowHeight / 2) + "px";

            //ライトボックスの表示位置を設定
            $("#side-l .selectwindow").css("top", selectwindowTop + "px");
            $("#side-l .fukiarrow").css("top", fukiarrowTop);
        }

        function scrollWindow() {
            //見出しがウィンドウ領域からはみ出たときの挙動
            documentTop = $(document).scrollTop();
            if (documentTop > selectwindowTop) {
                $("html,body").animate({
                    scrollTop: selectwindowTop + "px"
                }, 500, "easeOutQuint");
            }
        }

        //ライトボックスのHTMLを挿入
        $("#side-l").append(html);

        //現在のURL(「#」の削除)
        var url = this.href.replace(/#/, "");
        // var url = '/rental/-/assist/ajax-more/=/';
        //「○○で絞り込む」のテキスト部分
        var index   = url.indexOf('/=/');
        url         = '/rental/-/search/ajax-more/' + url.slice(index + 1);
        var text = $('#' + this.id + '_text').text();

        var time = parseInt( new Date() /1000 );

        $.ajax({

            type: "GET",
            url: url+"act=more/getnavi="+this.id+"/time="+time+"/",
            success: function(data){

                //getが成功したらHTMLに埋め込み
                $("#side-l #selectcontent").append(data);
                $("#side-l .selectwindow .capt .ttl").html(text);

                //背景を白くする処理
                $("body").append('<div id="whitescreen"></div>');
                $("#whitescreen").css("height",$("body").height() + "px").css("opacity",0).animate({
                        opacity: 0.5
                    }, 300, "easeOutQuint");

                //表示アニメーション（透過pngがうまくいかないのでIEはopacity処理なし）
                if (!$.support.opacity) {
                    $("#side-l .selectwindow").css("display", "block").animate({
                        left: "180px"
                    }, 300, "easeOutQuint");
                } else {
                    $("#side-l .selectwindow").css("display", "block").css("opacity",0).animate({
                        opacity: 1,
                        left: "180px"
                    }, 300, "easeOutQuint");
                }

                //ライトボックスの表示位置やはみ出たときの関数を実行
                setWindowPosition();
                scrollWindow();

                //ロードアイコンをはずす
                setTimeout(function () {
                    that.removeClass("load");
                }, 400);

            }

        });

        //閉じるボタン、領域外クリックでポップアップ削除
        $("#side-l .selectwindow .capt").click(function () {
            return false; //ポップアップクリックでは何も起こらない
        });
        $("#side-l .selectwindow .close").click(function () {
            $("#side-l .selectwindow").remove();
            $("#whitescreen").remove();
            flag = false;
        });
        $("body").click(function () {
            $("#side-l .selectwindow").remove();
            $("#whitescreen").remove();
            flag = false;
        });

        //aのデフォ挙動を取り消し
        return false;
    });





//ライトボックスのHTML
var html = '<div id="selectcontent" class="selectwindow"></div>';

}));
</script></div>
<!-- /su --></td>
