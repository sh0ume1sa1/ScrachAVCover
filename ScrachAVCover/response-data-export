<script type="text/javascript">
$('div.othertxt a.suggested').bind("click", function() {
  if(typeof i3 === "function") {
    var url = $(this).attr('href'),
        tracker = 'moshikashite_suggest',
        type = 'moshikashite_suggest',
        word = $(this).text(),
        rank = $('div.othertxt a.suggested').index(this) + 1;
    var set = {
          rank: rank,
          word: word,
          type: type
        },
        add = {
          suggest_rank: rank,
          suggest_word: word,
          suggest_type: type
        };
    i3('create', tracker);
    i3(tracker + '.set', 'suggest', set);
    i3(tracker + '.add', 'suggest', add);
    i3(tracker + '.send', 'click', 'suggest', function() {
      location.href = url;
    });
    return false;
  }
});

var getService = 'rental';
$('#list2 a').one("click contextmenu ", function(){
    var i = $('#list2 a').index(this);
    var link = $(this).attr('href');
    if (!link.match(/dmmref=bestseller/g)) {
        link = link+'?dmmref=bestseller';
        if (getService === 'digital') {
            if (!link.match(/i3_ref=recommend&i3_ord=/g)) {
                var position = parseInt(i) + 1;
                link = link+'&i3_ref=recommend&i3_ord='+position;
            }
        }
        $(this).attr('href',link);
    }
});
</script>

<!-- /mu --></td>
</tr>
</table>

