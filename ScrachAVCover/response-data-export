function loading_h_113sw080r() {
    $('div#h_113sw080r').html('<img src="//p.dmm.co.jp/p/rental/detail/ico_load.gif" alt="loading" width="12" height="12">');
}

$(function() {
    $(document).on('click','a#h_113sw080r', function() {
        var node = $(this).parent();
        var cid  = node.attr('cid');
        node.html('<img src="//p.dmm.co.jp/p/rental/detail/ico_load.gif" alt="loading" width="12" height="12">')
            .load('/rental/-/wishlist_add/=/lending_check=1/border=2/addPosition=list/pid=' + node.attr('id') + '/');
        ga('mutual.send', 'event', 'pc_adult_rental' , 'click', 'add_to_list');
        sendI3Log('click', new Array(cid), 'wish');
        return false;
    });
});
    //-->
